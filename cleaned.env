APP_NAME=Multi-Agent Code Generation System
APP_VERSION=1.0.0
DEBUG=false
HOST=0.0.0.0
PORT=8000
WORKERS=4
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/multiagent
DATABASE_ECHO=false
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=300
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production-min-32-chars
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=1440
API_KEY=your-api-key-for-legacy-auth
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.7
CREWAI_VERBOSE=true
CREWAI_MEMORY=true
MAX_ITER=10
MAX_EXECUTION_TIME=300
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760
ALLOWED_EXTENSIONS=[".py",".js",".ts",".jsx",".tsx",".html",".css",".json",".md",".txt",".yml",".yaml",".toml",".ini",".env"]
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600
WEBSOCKET_TIMEOUT=60
WEBSOCKET_PING_INTERVAL=30
ENABLE_PROMETHEUS=true
METRICS_ENDPOINT=/metrics
CORS_ORIGINS=["*"]
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=["*"]
CORS_ALLOW_HEADERS=["*"]
BACKEND_URL=http://localhost:8000
